{
  "name": "Musicalypse",
  "description": "A modern audio player built with Web technologies.",
  "version": "0.5.1",
  "author": "Thomas Gambet <thomas.gambet@gmail.com> (https://github.com/tgambet/)",
  "license": "MIT",
  "main": "target/electron/out-tsc/main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:serve:electron": "ng serve --configuration=electron",
    "ng:build": "ng build",
    "ng:build:prod": "ng build --prod",
    "ng:build:electron": "ng build --configuration=electron --output-path=target/dist/electron",
    "ng:test": "ng test",
    "ng:lint": "ng lint",
    "ng:e2e": "ng e2e",
    "sbt:run": "sbt run",
    "sbt:clean": "sbt clean",
    "sbt:compile": "sbt compile",
    "sbt:stage": "sbt stage",
    "web:run": "npm-run-all ng:build:prod sbt:run",
    "web:serve": "npm-run-all --parallel ng:serve sbt:run",
    "web:serve:electron": "npm-run-all --parallel ng:serve:electron sbt:run",
    "web:build": "npm-run-all ng:build:prod sbt:compile",
    "web:stage": "npm-run-all ng:build:prod sbt:stage",
    "electron:run": "npm-run-all electron:build && electron .",
    "electron:run:dev": "npm run electron:tsc && electron .",
    "electron:serve": "npm run electron:tsc && npm-run-all --parallel web:serve:electron electron:serve_",
    "electron:serve_": "wait-on http-get://localhost:4200 && electron . --serve",
    "electron:build": "npm-run-all --parallel electron:tsc ng:build:electron web:stage",
    "electron:tsc": "tsc --project electron",
    "electron:jre": "node bin/copy-jre.js",
    "electron:stage": "npm-run-all electron:build electron:windows",
    "electron:windows": "npx electron-builder build --windows --config build/electron/electron-builder.json",
    "electron:windows:jre": "npx electron-builder build --windows --config build/electron/electron-builder-jre.json",
    "electron:windows:store": "node bin/electron-windows-store.js",
    "electron:linux": "npx electron-builder build --linux --config build/electron/electron-builder-jre.json",
    "postinstall": "electron-builder install-app-deps",
    "heroku-postbuild": "npm run ng:build:prod"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/animations": "^6.0.7",
    "@angular/cdk": "^6.3.1",
    "@angular/cli": "~6.0.8",
    "@angular/common": "^6.0.7",
    "@angular/compiler": "^6.0.7",
    "@angular/compiler-cli": "^6.0.7",
    "@angular/core": "^6.0.7",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.7",
    "@angular/language-service": "^6.0.7",
    "@angular/material": "^6.3.1",
    "@angular/platform-browser": "^6.0.7",
    "@angular/platform-browser-dynamic": "^6.0.7",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.7",
    "@angular/service-worker": "^6.0.7",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.104",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "core-js": "^2.5.4",
    "dotenv": "^5.0.1",
    "electron": "^2.0.3",
    "electron-builder": "^20.19.1",
    "electron-reload": "^1.2.2",
    "electron-windows-store": "^0.12.0",
    "fs-extra": "^6.0.1",
    "hammerjs": "^2.0.8",
    "immutable": "^4.0.0-rc.9",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "npm-run-all": "^4.1.2",
    "prompt-sync": "^4.1.6",
    "protractor": "~5.3.2",
    "rxjs": "^6.2.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "wait-on": "^2.1.0",
    "zone.js": "^0.8.24"
  },
  "engines": {
    "node": "8.11.1",
    "npm": "5.7.1"
  }
}
